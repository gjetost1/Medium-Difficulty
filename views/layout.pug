extends navbar

block head
    style
        include ../public/stylesheets/layout.css

block layout
    div(class="layout__div__container")
        div(class="container__layout")
            block content
    div(class='layout__div__design')
    div(class='layout__div__circleParent')
        div(class='layout__div__circle')
            div(class='layout__div__curvedLine')
        div(class='layout__div__column')
            div(class='layout__div__innerPad')
            div(class='layout__div__outerPad')
            div(class='layout__div__rightAngle')
    div(class="layout__div__gamePad")
        div(class="layout__div__gamePad__buttonPad")
            div(class="layout__div__gamePad__button1")
            div(class="layout__div__gamePad__button2")
            div(class="layout__div__gamePad__button3")
            div(class="layout__div__gamePad__button4")
